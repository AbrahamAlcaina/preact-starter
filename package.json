  "author": {
    "name": "Luke Edwards",
    "email": "luke.edwards05@gmail.com",
    "url": "https://lukeed.com"
  },
  "scripts": {
    "build": "webpack --env.production --config config/webpack",
    "analize": "webpack --env.production --config config/webpack --profile --json > stats.json",
    "size": "webpack-bundle-analyzer stats.json",
    "start": "serve dist -s",
    "watch": "webpack-dashboard -- webpack-dev-server --config config/webpack",
    "server-pro": "better-npm-run server-pro",
    "server": "nodemon --watch server --watch dist server/index.js"
  },
  "betterScripts": {
    "server-pro": {
      "command": "node server/index.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "dependencies": {
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-register": "^6.11.6",
    "babel-runtime": "^6.23.0",
    "better-npm-run": "^0.0.15",
    "compression": "^1.6.2",
    "glob": "^7.1.1",
    "md-colors": "^1.0.0",
    "normalizr": "^3.2.2",
    "preact": "^7.2.0",
    "preact-redux": "^2.0.1",
    "preact-render-to-string": "^3.6.1",
    "preact-router": "^2.4.1",
    "redux": "^3.6.0",
    "redux-actions": "^2.0.2",
    "redux-saga": "^0.15.0",
    "reselect": "^3.0.0",
    "spdy": "^3.4.4",
    "spdy-push": "^1.0.2"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.2",
    "babel-core": "^6.24.1",
    "babel-loader": "^6.4.1",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-react-jsx": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-babili": "0.0.12",
    "babel-preset-env": "^1.4.0",
    "clean-webpack-plugin": "^0.1.15",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.27.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "html-webpack-plugin": "^2.28.0",
    "lodash-webpack-plugin": "^0.11.3",
    "node-sass": "^4.5.0",
    "nodemon": "^1.11.0",
    "postcss-loader": "^1.2.2",
    "remote-redux-devtools": "^0.5.7",
    "sass-loader": "^6.0.3",
    "serve": "^5.0.2",
    "style-loader": "^0.13.1",
    "sw-precache-webpack-plugin": "^0.9.0",
    "webpack": "^2.4.1",
    "webpack-bundle-analyzer": "^2.4.0",
    "webpack-dashboard": "^0.3.0",
    "webpack-dev-server": "^2.3.0"
  }
